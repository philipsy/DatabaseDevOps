USE Test

DECLARE @Msg nvarchar(4000); DECLARE @ErrLevel int
SET @Msg = 'Test for Rpt Schema'
IF EXISTS(
			SELECT Null FROM sys.schemas 
			WHERE name Like 'Rpt'
		 )
	SET @ErrLevel = 10
ELSE
	SET @ErrLevel = 16
RAISERROR(@Msg, @ErrLevel, 1)

SET @Msg = 'Test for RptTest  Schema'
IF EXISTS(
			SELECT Null FROM sys.schemas 
			WHERE name Like 'RptTest'
		 )
	SET @ErrLevel = 10
ELSE
	SET @ErrLevel = 16
RAISERROR(@Msg, @ErrLevel, 1)
